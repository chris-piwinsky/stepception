{
  "Comment": "A simple Step Function with 2 Lambda calls",
  "StartAt": "SubStep",
  "States": {
    "SubStep": {
      "Type": "Task",
      "Resource": "arn:aws:states:::states:startExecution",
      "Parameters": {
        "StateMachineArn": "${steparn}",
        "Input": {
          "NeedCallback": false,
          "AWS_STEP_FUNCTIONS_STARTED_BY_EXECUTION_ID.$": "$$.Execution.Id"
        }
      },
      "Next": "Lambda1"
    },
    "Lambda1": {
      "Type": "Task",
      "Resource": "${lambda1}",
      "End": true
    }
  }
}
